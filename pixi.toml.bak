[project]
name = "AutoML"
version = "0.1.0"
description = "Add a short description here"
authors = ["JoshuaSiraj <joshua.siraj@ryerson.ca>"]
channels = ["conda-forge", "pytorch"]
platforms = ["linux-64", "win-64"]

[tasks]
example = "jupyter lab"

[dependencies]
python = ">=3.10,<3.12"
pyyaml = ">=6.0.2,<7"
fpdf2 = ">=2.8.1,<3"
jupyterlab = ">=4.2.5,<5"
seaborn = ">=0.13.2,<0.14"
"autogluon.tabular" = ">1.0"
pandas = ">2.0"
scikit-learn = ">=1.2.2,<1.5"
openpyxl = ">=3.1.5,<3.2"
htmlmin = "==0.1.12"
scipy = ">=1.11.4,<1.13"
tableone = "==0.9.1"
borb = ">=2.1.25,<3"

[system-requirements]
linux= "5.4.0"

[pypi-dependencies]
pyyaml = "*"
mrmr-selection = "==0.2.8"
seismometer = ">=0.2.1, <0.3"
umap-learn = ">=0.5.6, <0.6"
shap = ">=0.46"
